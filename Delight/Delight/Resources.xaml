<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cm="clr-namespace:Delight.Commands"
    xmlns:local="clr-namespace:Delight">
    <ContextMenu x:Key="TrackItemContextMenu">
        <MenuItem Command="{x:Static cm:TrackItemCommands.DeleteCommand}" Header="이 아이템을 삭제(_D)" />
        <Separator />
    </ContextMenu>

    <ContextMenu x:Key="TrackContextMenu">
        <MenuItem Command="{x:Static cm:TrackCommands.DeleteCommand}" Header="트랙 삭제 (_D)" />
        <MenuItem Command="{x:Static cm:TrackCommands.AddCommand}" Header="트랙 추가 (_A)" />
    </ContextMenu>
</ResourceDictionary>